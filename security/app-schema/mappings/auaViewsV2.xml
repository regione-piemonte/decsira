<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns3:AppSchemaDataAccess xmlns:ns2="http://www.opengis.net/ogc" xmlns:ns3="http://www.geotools.org/app-schema">
    <namespaces>
        <Namespace>
            <prefix>sira</prefix>
            <uri>http://www.regione.piemonte.it/ambiente/sira/1.0</uri>
        </Namespace>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml/3.2</uri>
        </Namespace>
    </namespaces>
    <includedTypes>
        <Include xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">includedTypesViewsV2.xml</Include>
    </includedTypes>
    <sourceDataStores>
        <DataStore>
            <id>dataStore</id>
            <parameters>
                <Parameter>
                    <name>Connection timeout</name>
                    <value>20</value>
                </Parameter>
                <Parameter>
                    <name>port</name>
                    <value>${db.port}</value>
                </Parameter>
                <Parameter>
                    <name>passwd</name>
                    <value>${db.password}</value>
                </Parameter>
                <Parameter>
                    <name>dbtype</name>
                    <value>postgis</value>
                </Parameter>
                <Parameter>
                    <name>encode functions</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>Evictor run periodicity</name>
                    <value>300</value>
                </Parameter>
                <Parameter>
                    <name>namespace</name>
                    <value></value>
                </Parameter>
                <Parameter>
                    <name>schema</name>
                    <value>${db.schema}</value>
                </Parameter>
                <Parameter>
                    <name>create database</name>
                    <value>false</value>
                </Parameter>
                <Parameter>
                    <name>fetch size</name>
                    <value>1000</value>
                </Parameter>
                <Parameter>
                    <name>preparedStatements</name>
                    <value>false</value>
                </Parameter>
                <Parameter>
                    <name>min connections</name>
                    <value>1</value>
                </Parameter>
                <Parameter>
                    <name>Evictor tests per run</name>
                    <value>3</value>
                </Parameter>
                <Parameter>
                    <name>host</name>
                    <value>${db.host}</value>
                </Parameter>
                <Parameter>
                    <name>validate connections</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>max connections</name>
                    <value>10</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${db.name}</value>
                </Parameter>
                <Parameter>
                    <name>Support on the fly geometry simplification</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>Max connection idle time</name>
                    <value>300</value>
                </Parameter>
                <Parameter>
                    <name>Test while idle</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>Loose bbox</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>Expose primary keys</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>Max open prepared statements</name>
                    <value>50</value>
                </Parameter>
                <Parameter>
                    <name>Estimated extends</name>
                    <value>true</value>
                </Parameter>
                <Parameter>
                    <name>user</name>
                    <value>${db.user}</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>sipra.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>dataStore</sourceDataStore>
            <sourceType>v_sede</sourceType>
            <targetElement>sira:Sede</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>sira:tipoSede</targetAttribute>
                    <sourceExpression>
                        <OCQL>codice_sira</OCQL>
                        <linkElement>sira:TipoSede</linkElement>
                        <linkField>FEATURE_LINK[1]</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:codiceSira</targetAttribute>
                    <sourceExpression>
                        <OCQL>codice_sira</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:comune</targetAttribute>
                    <sourceExpression>
                        <OCQL>comune</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:depuratore</targetAttribute>
                    <sourceExpression>
                        <OCQL>flg_depuratore</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:suap</targetAttribute>
                    <sourceExpression>
                        <OCQL>fk_suap</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:provincia</targetAttribute>
                    <sourceExpression>
                        <OCQL>provincia</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:Sede</targetAttribute>
                    <idExpression>
                        <OCQL>strConcat('sede.', codice_sira)</OCQL>
                    </idExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:geometria</targetAttribute>
                    <sourceExpression>
                        <OCQL>geometria</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>FEATURE_LINK[1]</targetAttribute>
                    <sourceExpression>
                        <OCQL>id_istanza</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
        <FeatureTypeMapping>
            <sourceDataStore>dataStore</sourceDataStore>
            <sourceType>v_aua_v2</sourceType>
            <targetElement>sira:AutorizzazioneUnicaAmbientale</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>sira:impianto</targetAttribute>
                    <sourceExpression>
                        <OCQL>id_istanza</OCQL>
                        <linkElement>sira:Sede</linkElement>
                        <linkField>FEATURE_LINK[1]</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:impianto</targetAttribute>
                    <sourceExpression>
                        <OCQL>id_aua</OCQL>
                        <linkElement>sira:TipoImpianto</linkElement>
                        <linkField>FEATURE_LINK[1]</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:dettaglio</targetAttribute>
                    <sourceExpression>
                        <OCQL>id_aua</OCQL>
                        <linkElement>sira:SchedaRifiuto</linkElement>
                        <linkField>FEATURE_LINK[1]</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:geometria</targetAttribute>
                    <sourceExpression>
                        <OCQL>geometria</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:AutorizzazioneUnicaAmbientale</targetAttribute>
                    <idExpression>
                        <OCQL>strConcat('aua.', id_aua)</OCQL>
                    </idExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:codiceFiscale</targetAttribute>
                    <sourceExpression>
                        <OCQL>cf_soggetto</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:dataRilascio</targetAttribute>
                    <sourceExpression>
                        <OCQL>data_rilascio</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>strConcat('istanza.', id_istanza)</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:nrProvvedimento</targetAttribute>
                    <sourceExpression>
                        <OCQL>nr_provvedimento</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:procedimento/sira:Procedimento/sira:codiceBdc</targetAttribute>
                    <sourceExpression>
                        <OCQL>codice_bdc</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:procedimento/sira:Procedimento/sira:descrizione</targetAttribute>
                    <sourceExpression>
                        <OCQL>des_procedimento</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:istanza/sira:IstanzaAutorizzativa/sira:procedimento/sira:Procedimento</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>strConcat('procedimento.', id_procedimento)</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:capacitaMaxStoccMC</targetAttribute>
                    <sourceExpression>
                        <OCQL>capacita_max_stocc_mc</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:capacitaMaxStoccT</targetAttribute>
                    <sourceExpression>
                        <OCQL>capacita_max_stocc_t</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>strConcat('rifiuto.', id_aua)</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:notaQuadroTecnico</targetAttribute>
                    <sourceExpression>
                        <OCQL>nota_quadro_tecnico</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:qtaTotRecupero</targetAttribute>
                    <sourceExpression>
                        <OCQL>qta_tot_recupero</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:tipoRichiesta/sira:TipoRichiesta/sira:descrizione</targetAttribute>
                    <sourceExpression>
                        <OCQL>des_tipo_richiesta</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:tipoRichiesta/sira:TipoRichiesta/sira:flag</targetAttribute>
                    <sourceExpression>
                        <OCQL>flg_richiesta</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:rifiuto/sira:Rifiuto/sira:tipoRichiesta/sira:TipoRichiesta</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>strConcat('tipo_richiesta.', id_tipo_richiesta)</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:attivita/sira:Attivita/sira:descrizione</targetAttribute>
                    <sourceExpression>
                        <OCQL>des_attivita</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>sira:attivita/sira:Attivita</targetAttribute>
                    <ClientProperty>
                        <name>gml:id</name>
                        <value>strConcat('attivita.', id_attivita)</value>
                    </ClientProperty>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</ns3:AppSchemaDataAccess>
