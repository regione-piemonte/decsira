<?xml version="1.0" encoding="UTF-8" standalone="no"?><schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:decsiraogc_stabilimenti="http://www.regione.piemonte.it/ambiente/decsiraogc_stabilimenti/1.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.regione.piemonte.it/ambiente/decsiraogc_stabilimenti/1.0" version="1.0">
    <import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../0/gml.xsd"/>
	
	
	<!-- elements -->
	<!-- qui estendono gml:AbstractObject o gml:AbstractFeature -->
	
	<!-- decsira_geo_stabilimento -->
	<element name="Stabilimento" substitutionGroup="gml:AbstractFeature" type="decsiraogc_stabilimenti:StabilimentoType"/>
	<!-- decsira_t_autorizzazione_amb -->
	<element name="AutorizzazioneAmbientale" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:AutorizzazioneAmbientaleType"/>
	
	<!-- decsira_t_tematica -->
	<element name="Tematica" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TematicaType"/>
	
	<!-- decsira_t_sottoattivita id= id_sottoattivita -->
	<element name="SottoAttivitaEmissiva" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:SottoAttivitaEmissivaType"/>
	
	<!-- decsira_t_dettaglio_attivita id=id_dettaglio_attivita -->
	<element name="DettaglioAttivita" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:DettaglioAttivitaType"/>
	
	<!-- decsira_t_azienda  id = id_imp_azienda-->
	<element name="Azienda" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:AziendaType"/>
	
	<!-- decsira_t_stabilimento_ateco id = id_stab_ateco -->
	<element name="AttivitaProduttiva" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:AttivitaProduttivaType"/>
	
	<!-- decsira_t_sede_attiv_tiposede - id =id_sede_attiv_ts -->
	<element name="TipologiaImpianto" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TipologiaImpiantoType"/>
	
	<!-- decsira_t_cert_ambientale id=id_cert_amb -->
	<element name="CertificazioneAmbientale" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:CertificazioneAmbientaleType"/>
	
	<!-- decsira_geo_oggetto_associato -->
	<element name="OggettoAssociato" substitutionGroup="gml:AbstractFeature" type="decsiraogc_stabilimenti:OggettoAssociatoType"/>
	
	<!-- decsira_t_sorgenti_rum -->
	<element name="VerificaSorgentiRumorose" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:VerificaSorgentiRumoroseType"/>
	
	<!-- decsira_t_attiv_bassa_rum -->
	<element name="AttivitaBassaRumorosita" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:AttivitaBassaRumorositaType"/>
	
	<!-- decsira_t_prelievo_acqua  -->
	<element name="PrelievoAcqua" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:PrelievoAcquaType"/>
	
	<!-- decsira_geo_depuratore  -->
	<element name="ImpiantoDiDepurazione" substitutionGroup="gml:AbstractFeature" type="decsiraogc_stabilimenti:ImpiantoDiDepurazioneType"/>
	
	<!-- decsira_t_comune_servito  -->
	<element name="ComuneServito" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:ComuneServitoType"/>
	
	<!-- decsira_t_trattamento_avanzato  -->
	<element name="TrattamentoAvanzato" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TrattamentoAvanzatoType"/>
	
	<!-- decsira_t_tipo_filtr_finale  -->
	<element name="TipoFiltrazioneFinale" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TipoFiltrazioneFinaleType"/>
	
	<!-- decsira_t_tipo_disinfezione  -->
	<element name="TipoDisinfezione" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TipoDisinfezioneType"/>
	
	<!-- decsira_t_refluo_industriale_rec  -->
	<element name="RefluoIndustrialeRecapitato" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:RefluoIndustrialeRecapitatoType"/>
	
	<!-- decsira_t_trattamento_fanghi  -->
	<element name="TrattamentoFanghi" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:TrattamentoFanghiType"/>
	
	<!-- decsira_t_sostanza_refluo_industriale  -->
	<element name="SostanzaRecapitataRefluiIndustriali" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:SostanzaRecapitataRefluiIndustrialiType"/>
	
	<!-- decsira_t_destinazione_fanghi  -->
	<element name="DestinazioneFanghi" substitutionGroup="gml:AbstractObject" type="decsiraogc_stabilimenti:DestinazioneFanghiType"/>
	
	
	<!-- types -->
	<!-- qui estendono gml:AbstractGMLType o gml:AbstractFeatureType -->
	
	<complexType name="StabilimentoType">
        <complexContent>
            <extension base="gml:AbstractFeatureType">
                <sequence>
				    
					<xs:element minOccurs="1" name="codiceSira" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="nome">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="istatComune">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="6"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="nomeComune">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					
					<xs:element minOccurs="1" name="istatProvincia">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="6"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="desProvincia">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="indirizzo">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="200"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="emailPec">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="industriaAgroAlimentare">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="10"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="ricircoloInterno">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="10"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="modalitaRicircolo">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="percentualeRicircolo" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="volumeAnnuoRicircolo" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="volumeGiornoRicircolo" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="idStato" type="xs:integer"/>
					
					<xs:element minOccurs="0" name="idDepuratore" type="xs:integer"/>
					
					<xs:element minOccurs="0" name="desStato">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="tipoProvvedimentoAttivo">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="tipoProvvedimentoStorico">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="tematica">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="idTipologiaImpianto">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="desTipologiaImpianto">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="abitantiEquivalentiTrattati" type="xs:decimal"/>
					
					<xs:element minOccurs="1" name="geometria" type="gml:GeometryPropertyType"/>
					
					<!-- innesto oggetti piu complessi -->
					
					<!-- qui ci vuole un join -->
					 <xs:element maxOccurs="unbounded" minOccurs="0" name="autorizzazioneAmbientale" type="decsiraogc_stabilimenti:AutorizzazioneAmbientalePropertyType"/>
					 
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="1" name="azienda" type="decsiraogc_stabilimenti:AziendaPropertyType"/>
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="0" name="attivitaProduttiva" type="decsiraogc_stabilimenti:AttivitaProduttivaPropertyType"/>
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="0" name="tipologiaImpianto" type="decsiraogc_stabilimenti:TipologiaImpiantoPropertyType"/>
					  <!-- qui ci vuole un join -->
					 <xs:element maxOccurs="unbounded" minOccurs="0" name="certificazioneAmbientale" type="decsiraogc_stabilimenti:CertificazioneAmbientalePropertyType"/>
					 <!-- ha un elemento GEO -->
					 <xs:element maxOccurs="unbounded" minOccurs="0" name="oggettoAssociato" type="decsiraogc_stabilimenti:OggettoAssociatoPropertyType"/>
					 <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="1" minOccurs="0" name="verificaSorgentiRumorose" type="decsiraogc_stabilimenti:VerificaSorgentiRumorosePropertyType"/>
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="0" name="attivitaBassaRumorosita" type="decsiraogc_stabilimenti:AttivitaBassaRumorositaPropertyType"/>
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="0" name="prelievoAcqua" type="decsiraogc_stabilimenti:PrelievoAcquaPropertyType"/>
					  <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="1" minOccurs="0" name="impiantoDiDepurazione" type="decsiraogc_stabilimenti:ImpiantoDiDepurazionePropertyType"/>
					
                </sequence>
            </extension>
        </complexContent>
    </complexType>
	
	
	<complexType name="AutorizzazioneAmbientaleType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
				
					<xs:element minOccurs="1" name="idAutorizzazioneAmbientaleSede">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="idAutorizzazioneAmbientale" type="xs:decimal"/>
					
					<xs:element minOccurs="1" name="idTipoProvvedimento" type="xs:decimal"/>
					
					<xs:element minOccurs="1" name="tipoProvvedimento">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="500"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="desTematica">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="estremiProvvedimento">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="0" name="dataProvvedimento" type="xs:date"/>
					
					<xs:element minOccurs="0" name="dataScadenza" type="xs:date"/>
					
					<xs:element minOccurs="0" name="idEnteCompetente" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="desEnteCompetente">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="200"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element minOccurs="1" name="codiceSira" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="idStato" type="xs:decimal"/>
					
					<xs:element minOccurs="0" name="desStato">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<!-- innesto gli oggetti piu complessi -->
					
					<!-- condizione di join mediante id_istanza -->
					 <xs:element maxOccurs="unbounded" minOccurs="1" name="tematica" type="decsiraogc_stabilimenti:TematicaPropertyType"/>
					 <!-- qui ci vuole un join -->
					  <xs:element maxOccurs="unbounded" minOccurs="0" name="sottoAttivitaEmissiva" type="decsiraogc_stabilimenti:SottoAttivitaEmissivaPropertyType"/>
					 <!-- qui ci vuole un join -->
					 <xs:element maxOccurs="unbounded" minOccurs="0" name="dettaglioAttivita" type="decsiraogc_stabilimenti:DettaglioAttivitaPropertyType"/>
					
                </sequence>
            </extension>
        </complexContent>
    </complexType>
	
	
	<complexType name="TematicaType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
				
					<xs:element minOccurs="1" name="identificativo">
                        <xs:simpleType>
