{
  "detailsConfig": {
    "cardTemplateConfigUrl": "assets/cardTemplate.config",
    "cardModelConfigUrl": "assets/cardModelConfig.json",
    "wfsUrl": "http://sira.csi.geo-solutions.it/geoserver/ows?service=WFS&version=1.1.0&request=GetFeature&typeName=sira:AutorizzazioneUnicaAmbientale"
  },
  "modelConfig": {
    "root": "/wfs:FeatureCollection/gml:featureMembers/sira:AutorizzazioneUnicaAmbientale",
    "config": [
      {
        "field": "id",
        "xpath": ["@gml:id"],
        "type": 2
      },
      {
        "field": "codice",
        "xpath": ["sira:impianto/sira:Sede/sira:codiceSira/text()"],
        "type": 1
      },
      {
        "field": "codicefisc",
        "xpath": ["sira:istanza/sira:IstanzaAutorizzativa/sira:codiceFiscale/text()"],
        "type": 2
      },
      {
        "field": "comune",
        "xpath": ["sira:impianto/sira:Sede/sira:comune/text()"],
        "type": 2
      },
      {
        "field": "autamb",
        "xpath": [
            "sira:rifiuto/sira:Rifiuto/sira:attivita/sira:Attivita/sira:descrizione/text()"
        ],
        "type": 2
      },    
      {
        "type": 6,
        "field": "geometry",
        "xpath": ["sira:geometria/text()"]
      }
    ]
  }
}